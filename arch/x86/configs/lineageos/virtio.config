# 9p
CONFIG_9P_FS=y
CONFIG_NET_9P=y
CONFIG_NET_9P_VIRTIO=y

# ATA
CONFIG_ATA=y
CONFIG_ATA_PIIX=y
CONFIG_SATA_AHCI=y

# Block device
CONFIG_BLK_DEV_SR=y

# DMA BUF
CONFIG_DMABUF_HEAPS_SYSTEM=y

# DRM
CONFIG_DRM_VIRTIO_GPU=y
CONFIG_DRM_VKMS=m

# EFI
CONFIG_EFI_STUB=y

# Filesystem
CONFIG_ISO9660_FS=y

# HW random
CONFIG_HW_RANDOM_VIRTIO=y

# Mouse
CONFIG_MOUSE_PS2=y

# Sound
CONFIG_SND_ENS1370=y
CONFIG_SND_HDA_GENERIC=y
CONFIG_SND_HDA_INTEL=y
CONFIG_SND_INTEL8X0=y
CONFIG_SND_VIRTIO=y

# VirtIO
CONFIG_VIRTIO_BALLOON=y
CONFIG_VIRTIO_BLK=y
CONFIG_VIRTIO_CONSOLE=y
CONFIG_VIRTIO_INPUT=y
CONFIG_VIRTIO_NET=y
CONFIG_VIRTIO_PCI=y
